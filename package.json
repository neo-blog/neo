    "meta-marked": "^0.3.3",
    "mime-types": "^2.0.10",
    "mkdirp": "^0.5.0",
    "rimraf": "^2.3.3",
    "slugify": "^0.1.1",
    "songbird": "^1.0.1",
    "underscore": "^1.8.3",
    "unescape": "^0.1.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "http-server": "^0.8.0",
    "stylus": "^0.52.4"
  },
