{
  "name": "neo",
  "version": "0.0.1",
  "description": "A simple static portfolio site generator.",
  "main": "index.js",
  "scripts": {
    "generate-styles": "stylus styles/styl/main.styl --out styles/css/",
    "start": "http-server -p 9292 && echo \"starting site preview at http://localhost:9292\" ",
    "generate": "npm run generate-styles && babel-node --optional strict --stage 1 -- index.js",
