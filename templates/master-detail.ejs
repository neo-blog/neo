<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta charset="utf-8">
<script src="../vendor/jquery/jquery-2.1.4.js"></script>
<link href="../vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
<link href="../styles/css/main.css" rel="stylesheet" type="text/css" />
<script src="../vendor/bootstrap/js/bootstrap.min.js"></script>
<title><%= pageTitle%></title>
</head>
<body class="neo-Body">
  <%- include partials/header.ejs %>
  <div class="container neo-Container neo-Container-paddedTop">
    <% for(var i=0; i < detailItems.length; i++) {%>
      <div class="neo-Panel">
	      <div class="neo-Panel-heading">
	        <div class="row">
	          <div class="col-xs-8 col-md-10">
	            <h3 class="neo-Panel-title"><%= detailItems[i].title %></h3>
	          </div>
	          <div class="col-xs-4 col-md-2">
	            <a href="<%= detailItems[i].moreInfoLink %>" class="neo-Panel-moreinfolink">Read More</a>
	          </div>
	        </div>        
	      </div>
	      <div class="neo-Panel-body">
	        <p>
	          <%= detailItems[i].shortDescription %>
	        </p>
	      </div>
		</div>        
    <% } %> 
  </div>
</body>
</html>